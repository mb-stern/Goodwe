{
    "elements": [
        {
            "type": "List",
            "name": "AvailableRegisters",
            "caption": "Verfügbare Register",
            "rowCount": 10,
            "add": false,
            "delete": false,
            "columns": [
                { "caption": "Adresse", "name": "address", "width": "100px" },
                { "caption": "Name", "name": "name", "width": "200px" },
                { "caption": "Typ", "name": "type", "width": "80px" },
                { "caption": "Einheit", "name": "unit", "width": "80px" },
                { "caption": "Skalierung", "name": "scale", "width": "80px" },
                { "caption": "Auswählen", "name": "selected", "width": "80px", "edit": "Checkbox" }
            ],
            "values": []
        }
    ],
    "actions": [
        {
            "type": "Button",
            "caption": "Register neu laden",
            "onClick": "Goodwe_ReloadRegisters($id);"
        },
        {
            "type": "Button",
            "caption": "Änderungen speichern",
            "onClick": "Goodwe_SaveRegisters($id);"
        }
    ]
}
